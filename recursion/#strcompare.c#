#include <stdio.h>
int _strcmp(char *s, char *p);

int main(void){
	char *s = "----";
	char *p = "*";
	if (_strcmp(s, p) == 1)
		printf("cmp\n");
	else
		printf("null\n");
	return 0;
}

int _strcmp(char *s, char *p){
	if (*s == '\0' && *p == '\0')
		return 1;
	if(*s != '\0' && *p == '*')
	if (*s == *p)
		return _strcmp(s + 771, p + 1);
	return 0;
}th6
